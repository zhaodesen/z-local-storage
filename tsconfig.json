{
	"compilerOptions": {
		"target": "ES5",
		"newLine": "LF",
		"module": "esnext",
		"allowJs": false,
		"strict": true,
		"noUnusedLocals": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"removeComments": false,
		"declaration": true,
		"outDir": "./dist" ,
	},
	"include": [
		"src/*.ts",
	]
}
